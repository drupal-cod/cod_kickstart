{
  "name": "acquia/blt-project",
  "description": "Project template for Drupal 8 sites built with BLT.",
  "type": "project",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": {
    "0": {
      "type": "package",
      "package": {
        "name": "drupal-cod/program",
        "type": "drupal-module",
        "version": "master",
        "source": {
          "url": "https://github.com/drupal-cod/program.git",
          "type": "git",
          "reference": "master"
        }
      }
    },
    "1": {
      "type": "package",
      "package": {
        "name": "drupal-cod/sponsors",
        "type": "drupal-module",
        "version": "master",
        "source": {
          "url": "https://github.com/drupal-cod/sponsors.git",
          "type": "git",
          "reference": "master"
        }
      }
    },
    "2": {
      "type": "package",
      "package": {
        "name": "drupal-cod/event",
        "type": "drupal-module",
        "version": "master",
        "source": {
          "url": "https://github.com/drupal-cod/event.git",
          "type": "git",
          "reference": "master"
        }
      }
    },
    "3": {
      "type": "package",
      "package": {
        "name": "drupal-cod/ticket",
        "type": "drupal-module",
        "version": "master",
        "source": {
          "url": "https://github.com/drupal-cod/ticket.git",
          "type": "git",
          "reference": "master"
        }
      }
    }
  },
    "require": {
      "acquia/blt": "^8.9",
      "drupal-cod/program": "dev-master",
      "drupal-cod/sponsors": "dev-master",
      "drupal-cod/event": "dev-master",
      "drupal-cod/ticket": "dev-master"
    },
    "require-dev": {},
    "extra": {
        "enable-patching": true,
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
    "merge-plugin": {
      "require": [
        "vendor/acquia/blt/composer.required.json",
        "vendor/acquia/blt/composer.suggested.json"
      ],
      "include": [
        "blt/composer.overrides.json"
      ],
      "merge-extra": true,
      "merge-extra-deep": true,
      "merge-scripts": true,
      "replace": true
      }
  }
}
