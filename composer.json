{
  "name": "drupal/cod",
  "description": "A tool for building interactive conference websites.",
  "type": "drupal-profile",
  "license": "GPL-2.0+",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "scripts": {
    "post-install-cmd": [
      "@composer drupal-scaffold"
    ],
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
  },
  "require": {
    "cweagans/composer-patches": "^1.5.0",
    "drupal/address": "~1.0",
    "drupal/date": "1.x-dev",
    "drupal/devel": "~1.0",
    "drupal/diff": "~1.0",
    "drupal/features": "^3.5",
    "drupal/flag": "~4.0",
    "drupal/field_collection": "3.x-dev",
    "drupal/field_group": "~1.0",
    "drupal/field_permissions": "~1.0",
    "drupal/inline_entity_form": "~1.0",
    "drupal/pathauto": "~1.0",
    "drupal/webform": "5.x-dev",
    "drupal/og": "1.x-dev",
    "drupal/panels": "^4.1",
    "drupal/page_manager": "4.x-dev",
    "drupal/quicktabs": "~3.0",
    "drupal/rules": "~3.0",
    "drupal/token": "~1.0"
  }
}
